    <!-- Le styles -->
    

    <style type="text/css">
  
      /* Custom container */
      .container-narrow {
        margin: 0 auto;
        max-width: 960px;
        padding-top: 20px;
        background-color:rgba(245,245,245,0.8);
        top: -10px;
        margin-bottom: 0px;
        padding-bottom: 0px;
        position: relative;
      }

      a{
        color: #000;
      }

     hr {
       max-width: 80%;
      margin-left: 10%;
      border-bottom: none;
      border-top: 2px solid #afafaf;
      }

      .nav-pills > .active > a, .nav-pills > .active > a:hover, .btn {
         background-color: #FF9473;
      }

      /* Main marketing message and sign up button */
      .jumbotron {
        margin: 20px 0;
        padding: 10px;
        text-align: center;
      }
      .jumbotron h1 {
        font-size: 68px;
        line-height: 1;
      }
      .jumbotron .btn {
        font-size: 21px;
        padding: 14px 24px;
      }
      .jumbotron .btn .i {
        margin-right: 20px;
      }
      .masthead h3{
        /*font-family: calibri;*/
        font-size: 28px;
        padding-left: 10px;
      }
      .muted{
        padding-left: 10px;
         font-family: 'Libre Baskerville', Georgia, Times, serif;
      }
      /* Supporting marketing content */
      .marketing {
        margin: 0 auto;

      }

      .btn{
        background-color: hsl(13, 100%, 40%) !important;
        background-repeat: repeat-x;
        filter: progid:DXImageTransform.Microsoft.gradient(startColorstr="#ff5728", endColorstr="#cc2c00");
        background-image: -khtml-gradient(linear, left top, left bottom, from(#ff5728), to(#cc2c00));
        background-image: -moz-linear-gradient(top, #ff5728, #cc2c00);
        background-image: -ms-linear-gradient(top, #ff5728, #cc2c00);
        background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #ff5728), color-stop(100%, #cc2c00));
        background-image: -webkit-linear-gradient(top, #ff5728, #cc2c00);
        background-image: -o-linear-gradient(top, #ff5728, #cc2c00);
        background-image: linear-gradient(#ff5728, #cc2c00);
        border-color: #cc2c00 #cc2c00 hsl(13, 100%, 35.5%);
        color: #fff !important;
        text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.29);
        -webkit-font-smoothing: antialiased;
      }

      .marketing .span6{
        padding: 10px;
      }
      .marketing p + h4 {
        margin-top: 28px;
      }

      h1, h2, h3, h4, h5, h6, .muted, a {
        font-family: 'Sanchez', Georgia, Times, serif;
      }
      h4{
        font-size: 24px;
      }

      i{
        margin-right: 5px;
      }

      p {
        font-family: 'Nobile', Helvetica, Arial, sans-serif;
        font-size: 14px;
        color: #888;
      }
      footer{
        text-align: center;
        padding-bottom: 10px;
      }

      @media (max-width: 480px){
        .muted{
          margin-left: 10px;
        }
        .container-narrow {
          min-width: 320px;
           left: -12px;
        }

      }

    </style>

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->
  </head>

  <body>

    <div class="container-narrow">
